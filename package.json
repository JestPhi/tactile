{
    "name": "@stella/tactile",
    "version": "1.0.0",
    "description": "A lightweight, touch-optimized React design system with utility-first styling",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./preview": {
            "types": "./dist/preview.d.ts",
            "import": "./dist/preview.mjs",
            "require": "./dist/preview.js"
        },
        "./tactile.css": "./dist/tactile.css"
    },
    "files": [
        "dist",
        "README.md",
        "COMPONENT_TEMPLATE.md",
        "CSS_VARIABLES.md"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch",
        "type-check": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "react",
        "design-system",
        "ui-components",
        "touch-optimized",
        "utility-css",
        "accessible",
        "mobile-first"
    ],
    "author": "",
    "license": "MIT",
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "dependencies": {
        "nanoid": "^5.1.6",
        "react-feather": "^2.0.10",
        "react-router-dom": "^6.30.1"
    },
    "devDependencies": {
        "@types/node": "^25.0.3",
        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "@vitejs/plugin-react": "^5.1.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "typescript": "^5.8.3",
        "vite": "^7.3.0",
        "vite-plugin-dts": "^4.5.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/tactile"
    },
    "publishConfig": {
        "access": "public"
    },
    "bugs": {
        "url": "https://github.com/yourusername/tactile/issues"
    },
    "homepage": "https://github.com/yourusername/tactile#readme"
}